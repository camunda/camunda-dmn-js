{
  "name": "camunda-dmn-js",
  "version": "0.1.0",
  "description": "Embeddable Camunda modeling distributions based on dmn-js",
  "main": "lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "all": "run-s lint test distro test:distro",
    "lint": "eslint .",
    "dev": "npm test -- --auto-watch --no-single-run",
    "test": "karma start karma.config.js",
    "distro": "rollup -c",
    "test:distro": "node tasks/test-distro.js",
    "start:base": "cross-env SINGLE_START=base-modeler npm run dev",
    "start:platform": "cross-env SINGLE_START=camunda-platform-modeler npm run dev",
    "start:cloud": "cross-env SINGLE_START=camunda-cloud-modeler npm run dev",
    "prepublishOnly": "run-s distro test:distro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camunda/camunda-dmn-js"
  },
  "keywords": [
    "dmn",
    "dmn13",
    "camunda",
    "zeebe",
    "modeling",
    "dmn-js"
  ],
  "author": {
    "name": "Maciej Barelkowski",
    "url": "https://github.com/barmac"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/align-to-origin": "^0.7.0",
    "@bpmn-io/properties-panel": "^0.10.2",
    "diagram-js": "^7.8.2",
    "diagram-js-origin": "^1.3.2",
    "dmn-js": "^11.1.2",
    "min-dash": "^3.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^3.0.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-bpmn-io": "^0.13.0",
    "execa": "^5.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-debug-launcher": "0.0.4",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.0",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "min-dom": "^3.1.3",
    "mocha": "^9.2.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^8.0.0",
    "raw-loader": "^4.0.2",
    "rollup": "^2.38.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "webpack": "^5.20.1"
  }
}
