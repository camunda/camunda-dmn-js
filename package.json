{
  "name": "camunda-dmn-js",
  "version": "0.4.0",
  "description": "Embeddable Camunda modeling distributions based on dmn-js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "run-s lint test distro test:distro",
    "lint": "eslint .",
    "dev": "npm test -- --auto-watch --no-single-run",
    "test": "karma start karma.config.js",
    "distro": "rimraf dist && rollup -c",
    "test:distro": "node tasks/test-distro.js",
    "start": "run-s start:base",
    "start:base": "cross-env SINGLE_START=base-modeler npm run dev",
    "start:platform": "cross-env SINGLE_START=camunda-platform-modeler npm run dev",
    "start:cloud": "cross-env SINGLE_START=camunda-cloud-modeler npm run dev",
    "prepare": "run-s distro",
    "prepublishOnly": "run-s test:distro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/camunda/camunda-dmn-js"
  },
  "keywords": [
    "dmn",
    "dmn13",
    "camunda",
    "zeebe",
    "modeling",
    "dmn-js"
  ],
  "author": {
    "name": "Maciej Barelkowski",
    "url": "https://github.com/barmac"
  },
  "contributors": [
    {
      "name": "bpmn.io contributors",
      "url": "https://github.com/bpmn-io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "@bpmn-io/align-to-origin": "^0.7.0",
    "camunda-dmn-moddle": "^1.1.0",
    "diagram-js": "^7.8.3",
    "diagram-js-origin": "^1.3.2",
    "dmn-js-drd": "^12.1.1",
    "min-dash": "^3.8.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@bpmn-io/properties-panel": "^0.15.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^3.1.0",
    "@testing-library/dom": "^8.13.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-inferno": "~5.0.0",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "dmn-js": "^12.1.1",
    "dmn-js-properties-panel": "^1.1.0",
    "dmn-js-shared": "^12.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-bpmn-io": "^0.13.0",
    "execa": "^5.1.1",
    "inferno": "~5.6.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^6.3.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-debug-launcher": "0.0.4",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-webpack": "^5.0.0",
    "min-dom": "^3.1.3",
    "mocha": "^9.2.2",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^8.0.0",
    "raw-loader": "^4.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0",
    "webpack": "^5.72.0"
  },
  "peerDependencies": {
    "@bpmn-io/properties-panel": "0.15.x",
    "dmn-js": "^12.1.1",
    "dmn-js-properties-panel": "^1.0.0",
    "inferno": "5.6.x"
  }
}
